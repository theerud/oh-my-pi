{
  "name": "@oh-my-pi/omp-stats",
  "version": "12.7.1",
  "description": "Local observability dashboard for pi AI usage statistics",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "omp-stats": "./src/index.ts"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./src/server": {
      "types": "./src/server.ts",
      "import": "./src/server.ts"
    },
    "./src/db": {
      "types": "./src/db.ts",
      "import": "./src/db.ts"
    }
  },
  "files": [
    "src",
    "public",
    "README.md"
  ],
  "keywords": [
    "ai",
    "observability",
    "metrics",
    "dashboard",
    "llm",
    "statistics"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "check": "tsgo -p tsconfig.json && tsgo -p tsconfig.client.json",
    "build": "bun run build.ts"
  },
  "author": "Can Bölük",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/can1357/oh-my-pi.git",
    "directory": "packages/stats"
  },
  "homepage": "https://github.com/can1357/oh-my-pi",
  "bugs": {
    "url": "https://github.com/can1357/oh-my-pi/issues"
  },
  "dependencies": {
    "@oh-my-pi/pi-ai": "workspace:*",
    "@oh-my-pi/pi-utils": "workspace:*",
    "chart.js": "4.5.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.564.0",
    "react": "^19.2.4",
    "react-chartjs-2": "5.3.1",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "postcss": "8.5.6",
    "tailwindcss": "4"
  },
  "engines": {
    "bun": ">=1.3.7"
  }
}
