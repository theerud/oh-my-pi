{
	"name": "@oh-my-pi/pi-natives",
	"version": "12.8.2",
	"description": "Native Rust functionality via N-API",
	"keywords": ["napi", "rust", "native", "grep", "text-processing"],
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		}
	},
	"files": [
		"src",
		"native"
	],
	"scripts": {
		"build:native": "bun scripts/build-native.ts",
		"dev:native": "bun scripts/build-native.ts --dev",
		"embed:native": "bun scripts/embed-native.ts",
		"check": "biome check . && tsgo -p tsconfig.json",
		"fix": "biome check --write --unsafe .",
		"test": "bun run build:native && bun test",
		"bench": "bun bench/grep.ts"
	},
	"author": "Can Bölük",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/can1357/oh-my-pi.git",
		"directory": "packages/natives"
	},
	"homepage": "https://github.com/can1357/oh-my-pi",
	"bugs": { "url": "https://github.com/can1357/oh-my-pi/issues" },
	"dependencies": {
		"@oh-my-pi/pi-utils": "workspace:*"
	},
	"devDependencies": {
		"@types/bun": "^1.3.9"
	},
	"engines": {
		"bun": ">=1.3.7"
	}
}
