{
	"name": "@oh-my-pi/pi-coding-agent",
	"version": "12.12.3",
	"description": "Coding agent CLI with read, bash, edit, write tools and session management",
	"type": "module",
	"bin": {
		"omp": "src/cli.ts"
	},
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		},
		"./hooks": {
			"types": "./src/extensibility/hooks/index.ts",
			"import": "./src/extensibility/hooks/index.ts"
		},
		"./sdk": {
			"types": "./src/sdk.ts",
			"import": "./src/sdk.ts"
		},
		"./prompts/*": "./src/prompts/*.md",
		"./capability/*": {
			"types": "./src/capability/*.ts",
			"import": "./src/capability/*.ts"
		},
		"./config/*": {
			"types": "./src/config/*.ts",
			"import": "./src/config/*.ts"
		},
		"./extensibility/*": {
			"types": "./src/extensibility/*.ts",
			"import": "./src/extensibility/*.ts"
		},
		"./extensibility/hooks/*": {
			"types": "./src/extensibility/hooks/*.ts",
			"import": "./src/extensibility/hooks/*.ts"
		},
		"./extensibility/extensions/*": {
			"types": "./src/extensibility/extensions/*.ts",
			"import": "./src/extensibility/extensions/*.ts"
		},
		"./session/*": {
			"types": "./src/session/*.ts",
			"import": "./src/session/*.ts"
		},
		"./internal-urls": {
			"types": "./src/internal-urls/index.ts",
			"import": "./src/internal-urls/index.ts"
		},
		"./internal-urls/*": {
			"types": "./src/internal-urls/*.ts",
			"import": "./src/internal-urls/*.ts"
		},
		"./patch/*": {
			"types": "./src/patch/*.ts",
			"import": "./src/patch/*.ts"
		},
		"./cli": {
			"types": "./src/cli.ts",
			"import": "./src/cli.ts"
		},
		"./*": {
			"types": "./src/*.ts",
			"import": "./src/*.ts"
		}
	},
	"files": [
		"src",
		"scripts",
		"docs",
		"examples",
		"README.md",
		"CHANGELOG.md"
	],
	"scripts": {
		"check": "tsgo -p tsconfig.json",
		"format-prompts": "bun scripts/format-prompts.ts",
		"build:binary": "cd ../.. && bun --cwd=packages/stats scripts/generate-client-bundle.ts && bun --cwd=packages/natives run embed:native && bun build --compile --define PI_COMPILED=true --root . ./packages/coding-agent/src/cli.ts --outfile packages/coding-agent/dist/omp && bun --cwd=packages/natives run embed:native --reset && bun --cwd=packages/stats scripts/generate-client-bundle.ts --reset",
		"generate-template": "bun scripts/generate-template.ts",
		"test": "bun test"
	},
	"dependencies": {
		"@mozilla/readability": "0.6.0",
		"@oh-my-pi/omp-stats": "workspace:*",
		"@oh-my-pi/pi-agent-core": "workspace:*",
		"@oh-my-pi/pi-ai": "workspace:*",
		"@oh-my-pi/pi-natives": "workspace:*",
		"@oh-my-pi/pi-tui": "workspace:*",
		"@oh-my-pi/pi-utils": "workspace:*",
		"@sinclair/typebox": "^0.34.48",
		"@xterm/headless": "^6.0.0",
		"ajv": "^8.18.0",
		"chalk": "^5.6.2",
		"diff": "^8.0.3",
		"file-type": "^21.3.0",
		"glob": "^13.0.3",
		"handlebars": "^4.7.8",
		"ignore": "^7.0.5",
		"linkedom": "^0.18.12",
		"marked": "^17.0.2",
		"node-html-parser": "^7.0.2",
		"puppeteer": "^24.37.3",
		"smol-toml": "^1.6.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@types/bun": "^1.3.9",
		"@types/ms": "^2.1.0",
		"ms": "^2.1.3"
	},
	"keywords": [
		"coding-agent",
		"ai",
		"llm",
		"cli",
		"tui",
		"agent"
	],
	"author": "Can Bölük",
	"contributors": ["Mario Zechner"],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/can1357/oh-my-pi.git",
		"directory": "packages/coding-agent"
	},
	"homepage": "https://github.com/can1357/oh-my-pi",
	"bugs": { "url": "https://github.com/can1357/oh-my-pi/issues" },
	"engines": {
		"bun": ">=1.3.7"
	}
}
