{
	"name": "@oh-my-pi/pi-ai",
	"version": "2.0.1337",
	"description": "Unified LLM API with automatic model discovery and provider configuration",
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./utils/json-parse": "./src/utils/json-parse.ts"
	},
	"bin": {
