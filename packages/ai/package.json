{
	"name": "@oh-my-pi/pi-ai",
	"version": "11.0.1",
	"description": "Unified LLM API with automatic model discovery and provider configuration",
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		},
		"./models": {
			"types": "./src/models.ts",
			"import": "./src/models.ts"
		},
		"./models.generated": {
			"types": "./src/models.generated.ts",
			"import": "./src/models.generated.ts"
		},
		"./stream": {
			"types": "./src/stream.ts",
			"import": "./src/stream.ts"
		},
		"./types": {
			"types": "./src/types.ts",
			"import": "./src/types.ts"
		},
		"./usage": {
			"types": "./src/usage.ts",
			"import": "./src/usage.ts"
		},
		"./storage": {
			"types": "./src/storage.ts",
			"import": "./src/storage.ts"
		},
		"./providers/*": {
			"types": "./src/providers/*.ts",
			"import": "./src/providers/*.ts"
		},
		"./utils/*": {
			"types": "./src/utils/*.ts",
			"import": "./src/utils/*.ts"
		}
	},
	"bin": {
		"pi-ai": "./src/cli.ts"
	},
	"files": [
		"src",
		"README.md"
	],
	"scripts": {
		"check": "tsgo -p tsconfig.json",
		"generate-models": "bun scripts/generate-models.ts",
		"test": "bun test"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.72.1",
		"@aws-sdk/client-bedrock-runtime": "^3.982.0",
		"@bufbuild/protobuf": "^2.11.0",
		"@connectrpc/connect": "^2.1.1",
		"@connectrpc/connect-node": "^2.1.1",
		"@google/genai": "^1.39.0",
		"@mistralai/mistralai": "^1.13.0",
		"@oh-my-pi/pi-utils": "workspace:*",
		"@sinclair/typebox": "^0.34.48",
		"@smithy/node-http-handler": "^4.4.9",
		"ajv": "^8.17.1",
		"ajv-formats": "^3.0.1",
		"chalk": "^5.6.2",
		"openai": "^6.17.0",
		"partial-json": "^0.1.7",
		"zod-to-json-schema": "^3.25.1"
	},
	"keywords": [
		"ai",
		"llm",
		"openai",
		"anthropic",
		"gemini",
		"unified",
		"api"
	],
	"author": "Mario Zechner",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/can1357/oh-my-pi.git",
		"directory": "packages/ai"
	},
	"engines": {
		"bun": ">=1.3.7"
	},
	"devDependencies": {
		"@types/node": "^25.2.0"
	}
}
