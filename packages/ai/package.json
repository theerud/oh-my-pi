{
	"name": "@oh-my-pi/pi-ai",
	"version": "12.9.0",
	"description": "Unified LLM API with automatic model discovery and provider configuration",
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		},
		"./models": {
			"types": "./src/models.ts",
			"import": "./src/models.ts"
		},
		"./models.json": {
			"types": "./src/models.json",
			"import": "./src/models.json"
		},
		"./stream": {
			"types": "./src/stream.ts",
			"import": "./src/stream.ts"
		},
		"./types": {
			"types": "./src/types.ts",
			"import": "./src/types.ts"
		},
		"./usage": {
			"types": "./src/usage.ts",
			"import": "./src/usage.ts"
		},
		"./storage": {
			"types": "./src/storage.ts",
			"import": "./src/storage.ts"
		},
		"./providers/*": {
			"types": "./src/providers/*.ts",
			"import": "./src/providers/*.ts"
		},
		"./utils/*": {
			"types": "./src/utils/*.ts",
			"import": "./src/utils/*.ts"
		}
	},
	"bin": {
		"pi-ai": "./src/cli.ts"
	},
	"files": [
		"src",
		"README.md"
	],
	"scripts": {
		"check": "tsgo -p tsconfig.json",
		"generate-models": "bun scripts/generate-models.ts",
		"test": "bun test"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.74.0",
		"@aws-sdk/client-bedrock-runtime": "^3.990.0",
		"@bufbuild/protobuf": "^2.11.0",
		"@connectrpc/connect": "^2.1.1",
		"@connectrpc/connect-node": "^2.1.1",
		"@google/genai": "^1.41.0",
		"@mistralai/mistralai": "^1.14.0",
		"@oh-my-pi/pi-utils": "workspace:*",
		"@sinclair/typebox": "^0.34.48",
		"@smithy/node-http-handler": "^4.4.10",
		"ajv": "^8.18.0",
		"ajv-formats": "^3.0.1",
		"chalk": "^5.6.2",
		"openai": "^6.22.0",
		"partial-json": "^0.1.7",
		"zod": "^4.3.6",
		"zod-to-json-schema": "^3.25.1"
	},
	"keywords": [
		"ai",
		"llm",
		"openai",
		"anthropic",
		"gemini",
		"unified",
		"api"
	],
	"author": "Can BÃ¶lÃ¼k",
	"contributors": ["Mario Zechner"],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/can1357/oh-my-pi.git",
		"directory": "packages/ai"
	},
	"homepage": "https://github.com/can1357/oh-my-pi",
	"bugs": {
		"url": "https://github.com/can1357/oh-my-pi/issues"
	},
	"engines": {
		"bun": ">=1.3.7"
	},
	"devDependencies": {
		"@types/bun": "^1.3.9"
	}
}
